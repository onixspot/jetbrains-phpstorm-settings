        <entry key="SQL" value="false" />
        <entry key="TypeScript" value="false" />
        <entry key="XHTML" value="false" />
        <entry key="XML" value="true" />
        <entry key="yaml" value="false" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_INTENTION_BULB" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledLanguages">
